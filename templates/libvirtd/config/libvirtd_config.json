{
    "command": "/usr/local/bin/container-scripts/libvirtd.sh",
    "config_files": [
        {
            "dest": "/etc/libvirt/libvirtd.conf",
            "owner": "root:root",
            "perm": "0644",
            "source": "/var/lib/config-data/merged/libvirtd.conf"
        },
        {
            "dest": "/etc/libvirt/qemu.conf",
            "owner": "root:root",
            "perm": "0644",
            "source": "/var/lib/config-data/merged/qemu.conf"
        },
        {
            "dest": "/etc/nova/migration/identity",
            "owner": "nova:nova",
            "perm": "0600",
            "source": "/var/lib/config-data/merged/identity"
        }
    ]
}
